-- Scripts:

local function KKJZG_fake_script() -- Loading.LocalScript 

	local script = Instance.new('LocalScript', Loading)

	wait(1)

	script.Parent.Parent.Loading.Visible = true

	wait(1)

	script.Parent.firsttext.Visible = true

	wait(1)

	script.Parent.firsttext.Visible = false

	script.Parent.secondtext.Visible = true

	wait(1)

	script.Parent.firsttext.Visible = false

	script.Parent.secondtext.Visible = false

	script.Parent.three_txc.Visible = true

	wait(1)

	script.Parent.firsttext.Visible = false

	script.Parent.secondtext.Visible = false

	script.Parent.three_txc.Visible = false

	script.Parent.last_text.Visible = true

	wait(1)

	script.Parent.firsttext.Visible = false

	script.Parent.secondtext.Visible = false

	script.Parent.three_txc.Visible = false

	script.Parent.last_text.Visible = true

	script.Parent.Parent.Loading.Visible = false

	script.Parent.Parent.TheHack.Visible = true

end

coroutine.wrap(KKJZG_fake_script)()

local function LWEK_fake_script() -- TextButton.LocalScript 

	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect((function()

		script.Parent.Parent.apenabled.Visible = true

	end))

end

coroutine.wrap(LWEK_fake_script)()

local function SIBE_fake_script() -- TextButton_2.LocalScript 

	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Click:Connect((function()

		script.Parent.Parent.MP_ENABLED.Visible = true

	end))

end

coroutine.wrap(SIBE_fake_script)()

local function JZJI_fake_script() -- TextButton_3.LocalScript 

	local script = Instance.new('LocalScript', TextButton_3)

