coroutine.wrap(JZJI_fake_script)()

local function PIWP_fake_script() -- cklose.LocalScript 

	local script = Instance.new('LocalScript', cklose)

	script.Parent.MouseButton1Click:Connect((function()

		script.Parent.Parent.Parent["modmenu image button"].Visible = true

		script.Parent.Parent.Parent.TheHack.Visible = false

	end))

end

coroutine.wrap(PIWP_fake_script)()

local function YDQXMKZ_fake_script() -- modmenuimagebutton.LocalScript 

	local script = Instance.new('LocalScript', modmenuimagebutton)

	script.Parent.MouseButton1Click:Connect(function()

		script.Parent.Parent.TheHack.Visible = true

		script.Parent.Parent["modmenu image button"].Visible = false

	end)

end

coroutine.wrap(YDQXMKZ_fake_script)()
